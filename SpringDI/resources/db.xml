<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="oracle" class="com.di.db.MySQLDataService">
	<constructor-arg name="id" value="12345"/>
	<constructor-arg name="pass" value="9999"/>
	<property name="url" value="edu.ssafy.com.ora"/>
	<property name="driver" value="oracle_driver"/>
</bean>

<bean id="mysql" class="com.di.db.OracleDataService">
	<constructor-arg name="id" value="56789"/>
	<constructor-arg name="pass" value="1111"/>
	<property name="url" value="edu.ssafy.com.my"/>
	<property name="driver" value="mysql_drive"/>
</bean>
</beans>
